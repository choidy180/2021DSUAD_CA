<script>
  document.addEventListener("DOMContentLoaded", function(e){
    setTimeout(() => {
        document.querySelector('#start_event').style.display="none";
        document.querySelector('body').style.backgroundColor="#2979FF";
        document.querySelector('#MAIN_LOGO_IMG').style.visibility="visible";
    }, 2000);
  });
  var main_logo = document.querySelector('#MAIN_LOGO_IMG');
  const clientRect = main_logo.getBoundingClientRect();
  var topy = parseInt(window.pageYOffset + clientRect.top);
  var top1=false;
  var left1=false;
  function movey() {
    if(topy > -2000){
      main_logo.style.top=topy+"px";
      topy=topy-15;
    } else {
      top1=true;
    }
  }
  main_logo.addEventListener("click",(main_logo2)=>{
    main_logo.style.width="10%";
    main_logo.style.left="45%";
    var myVar = setInterval(myTimer, 4);
    function myTimer(){
      movey();
      if(top1){
        clearInterval(myVar);
        setTimeout(() => {
          document.querySelector("#window_body").style.backgroundColor="white";
          document.querySelector('#MAIN_BACK_DIV').style.visibility="hidden";
          document.querySelector('#MAIN_CONTAINER').style.backgroundColor="white";
          document.querySelector('#MAIN_CONTAINER2').style.display="flex";
          document.querySelector('#body_div').style.display="flex";
          document.body.style.overflowY="auto";
        }, 200);
      }
    }
  })
  document.addEventListener('scroll', function() {
    console.log(document.documentElement.scrollTop);
    if(document.documentElement.scrollTop > 100){
      document.querySelector('#MAIN_NAV').style.top="-100px";
      document.querySelector('#MAIN_NAV_IMG').style.top="-100px";
    } else {
      document.querySelector('#MAIN_NAV').style.top="0px";
      document.querySelector('#MAIN_NAV_IMG').style.top="50px";
    }
    // if(document.documentElement.scrollTop > 1600){
    //   document.querySelector('#MAIN_CONTAINER5').style.opacity="1";
    // }
  });
</script>

